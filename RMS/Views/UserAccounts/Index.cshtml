                @Html.DisplayFor(modelItem => item.DeletedByUserId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DateRestore)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RestoredByUserId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsActive)
            </td>
